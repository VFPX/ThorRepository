DEFINE CLASS form1 AS form 
 	*< CLASSDATA: Baseclass="form" Timestamp="" Scale="" Uniqueid="" />

	*-- OBJECTDATA items order determines ZOrder / El orden de los items OBJECTDATA determina el ZOrder 
	*< OBJECTDATA: ObjPath="MyMXBrowser" UniqueID="" Timestamp="" />
	*< OBJECTDATA: ObjPath="MyGrid" UniqueID="" Timestamp="" />

	*<DefinedPropArrayMethod>
		*m: writehtml
	*</DefinedPropArrayMethod>

	Caption = "Form1"
	Desktop = .T.
	DoCreate = .T.
	Height = 250
	Left = 0
	Name = "Form1"
	Top = 0
	Visible = .T.
	Width = 527

	ADD OBJECT 'MyGrid' AS grid WITH ;
		Height = 250, ;
		Left = 282, ;
		Name = "MyGrid", ;
		Top = 0, ;
		Width = 243
		*< END OBJECT: BaseClass="grid" />

	ADD OBJECT 'MyMXBrowser' AS peme_mxbrowser WITH ;
		Height = 250, ;
		Left = 0, ;
		Name = "MyMXBrowser", ;
		Top = 0, ;
		Width = 276
		*< END OBJECT: ClassLib="peme_mxbrowser.vcx" BaseClass="olecontrol" OLEObject="c:\windows\system32\ieframe.dll" Value="" />
	
	PROCEDURE Init
		Lparameters lcAlias, lcHTML, lnWidth, lnHeight, lnLeft, lnTop
		
		DoDefault()
		
		With This
			.Caption			 = lcAlias
			.MyGrid.RecordSource = lcAlias
			.WriteHtml (lcHTML)
		
			.Width	= Evl (lnWidth, .Width)
			.Height	= Evl (lnHeight, .Height)
		
			If [N] = Vartype (lnLeft)
				.Left = lnLeft
			Else
				.Left = (_vfp.Width - .Width) / 2
			Endif
		
			If [N] = Vartype (lnTop)
				.Top = lnTop
			Else
				.Top = (_vfp.Height - .Height) / 2
			Endif
		
		Endwith
		
	ENDPROC

	PROCEDURE writehtml
		Lparameters lcHTML
		
		This.MyMXBrowser.Write(lcHTML)
	ENDPROC

	PROCEDURE MyGrid.Init
		DoDefault()
		If Version(5) >= 900
			This.Anchor = 15
		EndIf 
		
	ENDPROC

	PROCEDURE MyMXBrowser.Init
		DoDefault()
		If Version(5) >= 900
			This.Anchor = 5
		EndIf 
		
	ENDPROC

ENDDEFINE
